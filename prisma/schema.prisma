generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  id        String  @id @default(cuid())
  key       String
  userId    String?
  extension String?
  url       String?
  type      String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("files")
}
